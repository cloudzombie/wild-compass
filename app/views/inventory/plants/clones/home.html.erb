<div role="tabpanel">
  <%= link_to 'Print PDF', inventory_plants_clones_download_path, class: 'btn btn-primary pull-right' %>

  <%#= link_to 'Print Monthly Report', inventory_download_path, class: 'btn btn-primary pull-right'%>
  
  <%= render 'inventory/nav' %>

  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="clones">
      <table class="table table-striped">
        <tr>
          <th>Name</th>
          <th>Strain</th>
        </tr>

        <% clones.each do |clone| %>
          <tr>
            <td><%= clone.try(:to_s) if clone.present? %></td>
            <td><%= strain_for clone.try(:strain) if clone.present? %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>