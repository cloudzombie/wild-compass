<div class="box box-solid box-primary">
  <div class="box-header">
    <h2 class="box-title">Weights</h2>
  </div>

  <div class="box-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Date</th>
          <th>Weight</th>
        </tr>
      </thead>

      <tbody>
        <% weights.each do |weight| %>
          <%= render weight %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
