<%= form_for(seed) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :stock, 'Quantity' %>
    <%= f.text_field :stock, class: 'form-control' %>
  </div>

  <div class="input-group">
    <%= f.submit class: 'btn btn-primary btn-flat' %>
  </div>
<% end %>
